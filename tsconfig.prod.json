{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": false,
    "target": "ES2020",
    "outDir": "bin",
    "esModuleInterop": true,
    "moduleResolution": "Node16",
    "module": "Node16",
    "baseUrl": "./src",
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "src/cfs.config.js", "src/cfs.config.cjs"]
}
