{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": false,
    "target": "ES2020",
    "outDir": "./",
    "esModuleInterop": true,
    "moduleResolution": "Node16",
    "module": "Node16",
    "baseUrl": "./src"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
