{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "./dist",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": false,
    "target": "ES2020",
    "outDir": "./dist",
    "esModuleInterop": true,
    "moduleResolution": "Node16",
    "module": "Node16",
    "baseUrl": "./src",
    "skipLibCheck": true
  },
  "include": ["src/**/*", "cfs.config.js"],
  "exclude": ["node_modules", "cfs.config.js", "src/cfs.config.cjs"]
}
